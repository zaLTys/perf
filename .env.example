# Environment Configuration for k6 Performance PoC
# Copy this file to .env and customize with your values
# NEVER commit .env to version control!

# Grafana Configuration
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=your-secure-password-here

# k6 Environment (dev, staging, prod)
K6_ENV=dev

# HTTP Client Retry Configuration
HTTP_RETRY_MAX_ATTEMPTS=3
HTTP_RETRY_INITIAL_DELAY=100
HTTP_RETRY_MAX_DELAY=5000
HTTP_RETRY_BACKOFF=2

# Auth Token (if using static token authentication)
# AUTH_TOKEN=your-auth-token-here

# Prometheus Remote Write (already configured in docker-compose)
K6_PROMETHEUS_RW_SERVER_URL=http://localhost:9090/api/v1/write
